    "log4js": "6.4.1",
    "morgan": "1.10.0",
    "serve-favicon": "2.4.5",
    "shelljs": "0.8.5",
    "socket.io": "4.4.1",
    "web3": "0.20.7"
  },
  "devDependencies": {
    "@hyperledger/cactus-api-client": "1.0.0",
